{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "no-duplicate-imports": 0,
    "react/prefer-stateless-function": 0,
    "react/sort-comp": 0,
    "import/imports-first": 0,
    "import/no-duplicates": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  }
}
