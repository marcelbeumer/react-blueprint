{
  "name": "react-blueprint",
  "version": "0.1.0",
  "description": "Ready for takeoff React setup",
  "license": "ISC",
  "main": "src/electron/main.js",
  "productName": "React Blueprint",
  "scripts": {
    "clean": "rimraf dist/* && rimraf build/*",
    "mocha": "mocha --compilers js:babel-register",
    "test": "npm run lint && flow && npm run mocha",
    "server": "node scripts/start-server.js",
    "render": "node scripts/render-pages.js",
    "dev-server": "node scripts/start-dev-server.js",
    "lint": "eslint src",
    "deploy": "git checkout master && npm run build && git checkout gh-pages && rimraf asset/* *.html && cp -r dist/* .",
    "build": "npm run clean && NODE_ENV=production webpack && NODE_ENV=production REVISION=`git rev-parse --short HEAD` npm run render -- --dist dist --asset dist/asset",
    "electron-package-darwin-x64": "npm run electron-build && mkdir -p build && electron-packager . --platform=darwin --arch=x64 --icon=./icon.icns --out build",
    "electron-build": "SSR=0 TARGET=electron npm run build",
    "electron-dev-server": "SSR=0 TARGET=electron npm run dev-server",
    "electron-dev": "ELECTRON_DEV_SERVER=1 npm run electron",
    "electron": "electron src/electron/main.js",
    "start": "npm run dev-server"
  },
  "homepage": "https://github.com/marcelbeumer/react-blueprint",
  "author": {
    "name": "Marcel Beumer",
    "email": "marcel@marcelbeumer.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:marcelbeumer/react-blueprint.git"
  },
  "bugs": {
    "url": "https://github.com/marcelbeumer/react-blueprint/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.3",
    "clean-css": "^3.4.16",
    "color": "^0.11.1",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.3",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.27.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "node-env": "^0.1.8",
    "normalize.css": "^4.1.1",
    "parallelshell": "^2.0.0",
    "path-match": "^1.2.4",
    "path-to-regexp": "^1.5.0",
    "penner": "^0.1.3",
    "postcss": "^5.0.21",
    "postcss-cssnext": "^2.6.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.9.1",
    "postcss-url": "^5.1.2",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hammerjs": "^0.4.5",
    "react-motion": "0.4.3",
    "redux": "^3.5.2",
    "redux-devtools": "^3.1.0",
    "redux-multi": "^0.1.91",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "rxjs": "^5.0.0-beta.8",
    "stilr": "^1.2.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "yargs": "^4.7.1"
  }
}
